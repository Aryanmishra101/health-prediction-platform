# Supabase Database Connection
# Django Settings
SECRET_KEY=your-secret-key-here
DEBUG=True
ALLOWED_HOSTS=localhost,127.0.0.1

# Database
DATABASE_URL=sqlite:///db.sqlite3
# For PostgreSQL: postgresql://user:password@localhost:5432/dbname

# Email Configuration (Optional)
EMAIL_HOST=smtp.gmail.com
EMAIL_PORT=587
EMAIL_HOST_USER=your-email@gmail.com
EMAIL_HOST_PASSWORD=your-app-password

# Sentry Error Tracking (Optional)
# Sign up at https://sentry.io for free error tracking
SENTRY_DSN=

# API Rate Limiting (Optional - defaults shown)
# ANON_RATE_LIMIT=100/hour
# USER_RATE_LIMIT=1000/hour
# PREDICTION_RATE_LIMIT=100/hour@aws-0-region.pooler.supabase.com:6543/postgres
